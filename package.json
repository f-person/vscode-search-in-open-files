{
  "name": "search-in-open-files",
  "displayName": "search in open files",
  "description": "lets u search in open files, files with git changes using the native vs code search",
  "version": "1.1.0",
  "publisher": "f-person",
  "repository": {
    "type": "git",
    "url": "https://github.com/f-person/vscode-search-in-open-files.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "search-in-open-files.searchOpenFiles",
        "title": "Search: Find in all Open Files"
      },
      {
        "command": "search-in-open-files.searchOpenFilesInActiveGroup",
        "title": "Search: Find in Files of the Active Tab Group"
      },
      {
        "command": "search-in-open-files.searchGitChangedFiles",
        "title": "Search: Find in all Files with Git Changes"
      },
      {
        "command": "search-in-open-files.searchStagedFiles",
        "title": "Search: Find in Staged Files"
      },
      {
        "command": "search-in-open-files.searchUnstagedFiles",
        "title": "Search: Find in Unstaged Files"
      }
    ],
    "keybindings": [
      {
        "command": "search-in-open-files.searchOpenFiles",
        "key": "ctrl+shift+g",
        "mac": "cmd+shift+g",
        "when": "editorFocus"
      },
      {
        "command": "search-in-open-files.searchGitChangedFiles",
        "key": "ctrl+shift+alt+g",
        "mac": "cmd+shift+alt+g",
        "when": "editorFocus"
      },
      {
        "command": "search-in-open-files.searchStagedFiles",
        "key": "ctrl+shift+alt+s",
        "mac": "cmd+shift+alt+s",
        "when": "editorFocus"
      },
      {
        "command": "search-in-open-files.searchUnstagedFiles",
        "key": "ctrl+shift+alt+u",
        "mac": "cmd+shift+alt+u",
        "when": "editorFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.100.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
